{
  "name": "felastrap",
  "private": true,
  "version": "1.0.0",
  "author": {
    "email": "vamsiampolu3000@gmail.com",
    "name": "Vamsi Ampolu",
    "url": "https://github.com/vamsiampolu"
  },
  "description": "A React component library built using fela, the atomic CSS-in-JS library",
  "license": "MIT",
  "type": "module",
  "main": "./dist/felastrap.cjs.js",
  "module": "./dist/felastrap.es.js",
  "types": "./dist/felastrap.es.d.ts",
  "exports": {
    ".": {
      "import": "./dist/felastrap.es.js",
      "require": "./dist/felastrap.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "format": "prettier --write .",
    "dev": "vite",
    "build": "tsc && vite build",
    "test:debug": "node --inspect-brk --experimental-vm-modules --expose-gc ./node_modules/.bin/jest --runInBand --logHeapUsage",
    "test:diag": "node --experimental-vm-modules --expose-gc ./node_modules/.bin/jest --runInBand --detectOpenHandles --logHeapUsage",
    "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "rimraf ./dist ./storybook-static"
  },
  "dependencies": {
    "@floating-ui/core": "^1.5.0",
